{
  "app": {
    "name": "My Application",
    "version": "1.0.0",
    "description": "My web application wrapper",
    "author": "Your Name",
    "homepage": "https://www.example.com"
  },
  "target": {
    "url": "https://www.example.com",
    "userAgent": null
  },
  "window": {
    "width": 1200,
    "height": 800,
    "minWidth": 800,
    "minHeight": 600,
    "maxWidth": null,
    "maxHeight": null,
    "resizable": true,
    "movable": true,
    "minimizable": true,
    "maximizable": true,
    "closable": true,
